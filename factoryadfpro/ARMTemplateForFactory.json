													"name": "cdc_timestamp",
													"value": {
														"value": "@activity('MaxCDC').output.resultSets[0].rows[0].last_modified",
														"type": "Expression"
													}
												}
											],
											"storeSettings": {
												"type": "AzureBlobFSReadSettings",
												"recursive": true,
												"enablePartitionDiscovery": false
											},
											"formatSettings": {
