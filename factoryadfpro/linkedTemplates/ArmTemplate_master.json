{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "factoryadfpro"
		},
		"adls_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls_ls'"
		},
		"db_ls_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'db_ls'"
		},
		"Ls_Rest_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://pokeapi.co/api/v2/pokemon"
		},
		"adls_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlspro.dfs.core.windows.net/"
		},
		"db_ls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "prodfserver.database.windows.net"
		},
		"db_ls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "praveendb"
		},
		"db_ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "praveenadmin"
		},
		"TriggerPipeline_properties_Scheduled_pipeline_parameters_p_schema": {
			"type": "string",
			"defaultValue": "dbo"
		},
		"TriggerPipeline_properties_Scheduled_pipeline_parameters_p_table": {
			"type": "string",
			"defaultValue": "Orderss"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "factoryadfpro_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adls_ls_accountKey": {
						"value": "[parameters('adls_ls_accountKey')]"
					},
					"db_ls_password": {
						"value": "[parameters('db_ls_password')]"
					},
					"Ls_Rest_properties_typeProperties_url": {
						"value": "[parameters('Ls_Rest_properties_typeProperties_url')]"
					},
					"adls_ls_properties_typeProperties_url": {
						"value": "[parameters('adls_ls_properties_typeProperties_url')]"
					},
					"db_ls_properties_typeProperties_server": {
						"value": "[parameters('db_ls_properties_typeProperties_server')]"
					},
					"db_ls_properties_typeProperties_database": {
						"value": "[parameters('db_ls_properties_typeProperties_database')]"
					},
					"db_ls_properties_typeProperties_userName": {
						"value": "[parameters('db_ls_properties_typeProperties_userName')]"
					},
					"TriggerPipeline_properties_Scheduled_pipeline_parameters_p_schema": {
						"value": "[parameters('TriggerPipeline_properties_Scheduled_pipeline_parameters_p_schema')]"
					},
					"TriggerPipeline_properties_Scheduled_pipeline_parameters_p_table": {
						"value": "[parameters('TriggerPipeline_properties_Scheduled_pipeline_parameters_p_table')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}